<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp"
    android:clickable="true"
    android:foreground="?attr/selectableItemBackground"
    >

    <LinearLayout
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="id"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="placas"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="modelo"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="marca"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="estado"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="ensambladora"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <View
            android:id="@+id/divider7"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/mostrar_id"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="mostrar_id"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_placas"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="mostrar_placas"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_modelo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="mostrar_modelo"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_marca"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="mostrar_marca"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_estado"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="mostrar_estado"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_ensabladora"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="sans-serif"
            android:text="mostrar_ensambladora"
            android:textAllCaps="false"
            android:textColorHighlight="#FFFFFF"
            android:textColorLink="#121111"
            android:textSize="10.5dp"
            android:typeface="normal" />

        <View
            android:id="@+id/divider6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

</LinearLayout>